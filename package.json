{
  "name": "ts-react-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "tcm": "tcm ./src -c -o ./typings",
    "prestart:dev": "yarn tcm",
    "start:dev": "webpack-dev-server --host 0.0.0.0",
    "prebuild": "yarn tcm",
    "build": "NODE_ENV=production webpack",
    "prebuild:dev": "yarn tcm",
    "build:dev": "webpack --watch --progress",
    "lint": "tslint src/**/*.ts src/**/*.tsx --format stylish",
    "fmt": "prettier --config .prettierrc --write src/** && tslint src/**/*.ts src/**/*.tsx --fix",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "devDependencies": {
    "@types/enzyme": "^3.9.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.12",
    "@types/puppeteer": "^1.12.3",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "@types/react-test-renderer": "^16.8.1",
    "autoprefixer": "^9.5.1",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "jest-css-modules-transform": "^2.1.1",
    "jest-puppeteer": "^4.1.1",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.17.0",
    "react-test-renderer": "^16.8.6",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typed-css-modules": "^0.4.2",
    "typed-css-modules-loader": "^0.0.17",
    "typescript": "^3.4.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "react": "^16.8.6",
    "react-css-modules": "^4.7.9",
    "react-dom": "^16.8.6"
